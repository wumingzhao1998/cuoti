{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": null,
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "installCommand": "pip install --upgrade pip && pip install -r requirements.txt --no-cache-dir"
}

